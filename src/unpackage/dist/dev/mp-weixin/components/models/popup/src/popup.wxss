.tn-popup.data-v-a4b4ea63 {
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  overflow: hidden;
  visibility: hidden;
  /* 弹出层内容 start */
  /* 弹出层内容 end */
  /* 关闭按钮 start */
  /* 关闭按钮 end */
}
.tn-popup__content.data-v-a4b4ea63 {
  position: absolute;
  opacity: 0;
  transition-property: transform, opacity;
  transition-duration: 0.3s;
  transition-timing-function: ease;
  /* 弹出层位置 start */
  /* 弹出层位置 end */
}
.tn-popup__content--center.data-v-a4b4ea63 {
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%) scale(0);
  transform-origin: center center;
}
.tn-popup__content--top.data-v-a4b4ea63 {
  left: 0;
  top: 0;
  width: 100%;
  transform: translateY(-100%);
  transform-origin: top center;
}
.tn-popup__content--left.data-v-a4b4ea63 {
  left: 0;
  top: 0;
  height: 100%;
  transform: translateX(-100%);
  transform-origin: left center;
}
.tn-popup__content--right.data-v-a4b4ea63 {
  right: 0;
  top: 0;
  height: 100%;
  transform: translateX(100%);
  transform-origin: right center;
}
.tn-popup__content--bottom.data-v-a4b4ea63 {
  left: 0;
  bottom: 0;
  width: 100%;
  transform: translateY(100%);
  transform-origin: bottom center;
}
.tn-popup__close-btn.data-v-a4b4ea63 {
  position: absolute;
  padding: 18rpx;
  font-size: 32rpx;
  line-height: 1;
  color: var(--tn-color-gray);
}
.tn-popup__close-btn--right-top.data-v-a4b4ea63 {
  right: 0rpx;
  top: 0rpx;
}
.tn-popup__close-btn--left-top.data-v-a4b4ea63 {
  left: 0rpx;
  top: 0rpx;
}
.tn-popup__close-btn--right-bottom.data-v-a4b4ea63 {
  right: 0rpx;
  bottom: 0rpx;
}
.tn-popup__close-btn--left-bottom.data-v-a4b4ea63 {
  left: 0rpx;
  bottom: 0rpx;
}
.tn-popup.is-show .tn-popup__content.data-v-a4b4ea63 {
  opacity: 1;
}
